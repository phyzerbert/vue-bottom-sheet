{
  "name": "@phyzerbert/vue-bottom-sheet",
  "version": "1.3.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phyzerbert/vue-bottom-sheet.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "component",
    "bottom-sheet"
  ],
  "author": "phyzerbert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phyzerbert/vue-bottom-sheet/issues"
  },
  "homepage": "https://github.com/phyzerbert/vue-bottom-sheet#readme",
  "size-limit": [
    {
      "path": "dist/vue-bottom-sheet.min.js",
      "limit": "5 KB"
    }
  ],
  "main": "dist/vue-bottom-sheet.ssr.js",
  "browser": "dist/vue-bottom-sheet.esm.js",
  "module": "dist/vue-bottom-sheet.esm.js",
  "unpkg": "dist/vue-bottom-sheet.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "size": "size-limit"
  },
  "dependencies": {
    "hammerjs": "^2.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@size-limit/preset-app": "^4.10.0",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "size-limit": "^4.10.1",
    "vue": "^2.6.12",
    "vue-color": "^2.8.1",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=12"
  }
}
